{
  "node_colors": {
    "entity": "#FF6B6B",
    "relationship": "#4ECDC4",
    "concept": "#45B7D1",
    "attribute": "#FFA07A"
  },
  "node_shapes": {
    "entity": "ellipse",
    "relationship": "box",
    "concept": "circle",
    "attribute": "diamond"
  },
  "layout": {
    "algorithm": "hierarchical",
    "spacing": 300.0,
    "hierarchical": true
  },
  "physics": {
    "enabled": true,
    "stabilization": true,
    "repulsion": 250.0,
    "spring_length": 200.0,
